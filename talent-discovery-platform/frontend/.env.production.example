# ==============================================
# Get-Noticed Frontend - PRODUCTION Environment
# ==============================================
# Copy this to .env.production and fill in your values
# NEVER commit .env.production to version control!
# ==============================================

# API Configuration
# Backend runs on same domain (proxied through nginx/cloudflare)
REACT_APP_API_URL=https://get-noticed.net/api/v1
REACT_APP_BACKEND_URL=https://get-noticed.net

# WebSocket URL for real-time features
REACT_APP_SOCKET_URL=wss://get-noticed.net

# ==============================================
# Google OAuth
# ==============================================
REACT_APP_GOOGLE_CLIENT_ID=833841619802-bv9mu96rtej5ahvk7d8bafnp1e4d50ep.apps.googleusercontent.com

# Google Analytics (optional)
# REACT_APP_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Sentry Error Tracking (optional but recommended)
# REACT_APP_SENTRY_DSN=https://your-sentry-dsn

# Feature Flags
REACT_APP_ENABLE_AI_FEATURES=true
REACT_APP_ENABLE_2FA=true

# App Info
REACT_APP_NAME=Get-Noticed
REACT_APP_VERSION=1.0.0
