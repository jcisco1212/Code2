{
  "name": "get-noticed-backend",
  "version": "1.0.0",
  "description": "Get-Noticed API Server - AI-Powered Talent Discovery Platform",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "ts-node --transpile-only src/index.ts",
    "start:prod": "node dist/index.js",
    "worker": "ts-node src/workers/index.ts",
    "worker:dev": "ts-node-dev --respawn --transpile-only src/workers/index.ts",
    "worker:prod": "node dist/workers/index.js",
    "db:migrate": "echo 'Database initialized via init.sql'",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "seed:categories": "ts-node src/scripts/seedCategories.ts",
    "seed:admin": "ts-node src/scripts/seedAdmin.ts",
    "db:add-user-columns": "ts-node src/scripts/addUserColumns.ts",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --runInBand",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "regenerate-thumbnails": "ts-node scripts/regenerate-thumbnails.ts",
    "resize-categories": "ts-node scripts/resize-category-images.ts",
    "resize-all-categories": "ts-node scripts/resize-all-category-images.ts",
    "fix-categories": "ts-node scripts/fix-category-images.ts"
  },
  "dependencies": {
    "@aws-sdk/client-mediaconvert": "^3.450.0",
    "@aws-sdk/client-s3": "^3.450.0",
    "@aws-sdk/client-ses": "^3.450.0",
    "@aws-sdk/cloudfront-signer": "^3.450.0",
    "@aws-sdk/s3-presigned-post": "^3.450.0",
    "@aws-sdk/s3-request-presigner": "^3.450.0",
    "aws-sdk": "^2.1500.0",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.0",
    "clamdjs": "^1.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "fluent-ffmpeg": "^2.1.2",
    "google-auth-library": "^10.5.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "otplib": "^12.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.3",
    "redis": "^4.6.11",
    "sequelize": "^6.35.2",
    "sharp": "^0.33.1",
    "socket.io": "^4.7.2",
    "speakeasy": "^2.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^3.0.13",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
